{
  "name": "@dxenv/cli",
  "version": "0.0.1",
  "description": "The best way to securely share and manage environment variables in your team.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "type": "module",
  "bin": {
    "dxenv": "./dist/index.js",
    "dx": "./dist/index.js"
  },
  "scripts": {
    "test": "",
    "dev": "dotenv -e .env.local -- tsx ./src/index.ts",
    "build": "rm -rf ./dist && tsup",
    "lint": "tsc"
  },
  "files": [
    "./dist/**",
    "./package.json",
    "./README.md"
  ],
  "keywords": [
    "real-time",
    "convex",
    "vault",
    "secrets",
    "management",
    "dotenv"
  ],
  "dependencies": {
    "@types/node": "^20.14.14",
    "async-listen": "^3.0.1",
    "axios": "^1.7.4",
    "chokidar": "^3.6.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "glob": "^11.0.0",
    "nanoid": "3.3.4",
    "open": "^10.1.0",
    "ora": "^8.0.1",
    "picocolors": "^1.0.1",
    "zx": "^8.1.4"
  },
  "devDependencies": {
    "esbuild": "^0.23.1",
    "tsup": "^8.2.4",
    "tsx": "^4.16.5"
  },
  "author": "Emmanuel Ajike",
  "license": "MIT"
}
